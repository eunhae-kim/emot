{
  "config": {
    "privateApi_dev": "https://s3-open-api-dev.s3.ap-northeast-2.amazonaws.com/public-apigw-oas.json",
    "publicApi_dev": "https://s3-open-api-dev.s3.ap-northeast-2.amazonaws.com/private-apigw-oas.json",
    "downloadLocalOpenApiSpecFrom": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v2.0/json/petstore.json",
    "downloadDevOpenApiSpecFrom": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v2.0/json/petstore.json",
    "downloadStgOpenApiSpecFrom": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v2.0/json/petstore.json",
    "downloadPrdOpenApiSpecFrom": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v2.0/json/petstore.json",
    "downloadOpenApiSpecDir": "./openapi-spec/download/",
    "downloadOpenApiSpecFilename": "spec.json",
    "openapiApiOutputPath": "./openapi-spec/ts"
  },
  "private": true,
  "scripts": {
    "gen-cert": "node ./src/cmdUtils/genCert.js",
    "dev": "npm-run-all build-common dev-next",
    "storybook": "npm-run-all build-common start-storybook",
    "build": "npm-run-all build-common build-next",
    "start-dd": "node --require ./server-preload.js ./node_modules/.bin/next start",
    "start": "next start",
    "start:local": "cross-env DEPLOY_ENV=LOCAL next start",
    "start:stg": "cross-env DEPLOY_ENV=STG next start",
    "start:green": "cross-env DEPLOY_ENV=GREEN next start",
    "start:blue": "cross-env DEPLOY_ENV=BLUE next start",
    "dev-next": "cross-env DEPLOY_ENV=LOCAL node server.js",
    "dev-css": "webpack -w --config customBuildCfg/css.webpack.config.js",
    "build-css": "webpack --config customBuildCfg/css.webpack.config.js",
    "build-comp": "webpack --config customBuildCfg/comp.webpack.config.js",
    "build-next": "next build",
    "build-common": "npm-run-all build-css",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:openapi-spec": "cross-var \"rimraf  $npm_package_config_openapiApiOutputPath && openapi-generator-cli generate -i $npm_package_config_downloadOpenApiSpecDir$npm_package_config_downloadOpenApiSpecFilename -g typescript-fetch -o $npm_package_config_openapiApiOutputPath --skip-validate-spec --additional-properties=npmName=restClient,supportsES6=true,withInterfaces=true,typescriptThreePlus=true\"",
    "download:local-openapi-spec": "cross-var \"mkdirp $npm_package_config_downloadOpenApiSpecDir && download $npm_package_config_downloadLocalOpenApiSpecFrom > $npm_package_config_downloadOpenApiSpecDir$npm_package_config_downloadOpenApiSpecFilename\"",
    "download:dev-openapi-spec": "cross-var \"mkdirp $npm_package_config_downloadOpenApiSpecDir && download $npm_package_config_downloadDevOpenApiSpecFrom > $npm_package_config_downloadOpenApiSpecDir$npm_package_config_downloadOpenApiSpecFilename\"",
    "download:stg-openapi-spec": "cross-var \"mkdirp $npm_package_config_downloadOpenApiSpecDir && download $npm_package_config_downloadStgOpenApiSpecFrom > $npm_package_config_downloadOpenApiSpecDir$npm_package_config_downloadOpenApiSpecFilename\"",
    "download:prd-openapi-spec": "cross-var \"mkdirp $npm_package_config_downloadOpenApiSpecDir && download $npm_package_config_downloadOpenApiSpecFrom > $npm_package_config_downloadOpenApiSpecDir$npm_package_config_downloadOpenApiSpecFilename\"",
    "update:local-openapi-spec": "npm-run-all download:local-openapi-spec build:openapi-spec",
    "update:dev-openapi-spec": "npm-run-all download:dev-openapi-spec build:openapi-spec",
    "update:stg-openapi-spec": "npm-run-all download:stg-openapi-spec build:openapi-spec",
    "update:prd-openapi-spec": "npm-run-all download:prd-openapi-spec build:openapi-spec"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "axios-cookiejar-support": "^4.0.3",
    "chart.js": "^3.6.0",
    "chartjs-plugin-annotation": "^1.4.0",
    "classnames": "^2.3.1",
    "core-js": "^3.24.1",
    "date-fns": "^2.11.1",
    "dd-trace": "^3.8.0",
    "dotenv": "^16.0.1",
    "focusable-selectors": "^0.8.0",
    "gray-matter": "^4.0.2",
    "gsap": "^3.11.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "latest",
    "next-cookies": "^2.0.3",
    "next-env": "^1.1.1",
    "react": "17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^9.4.0",
    "react-transition-group": "^4.4.5",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0",
    "swiper": "^8.3.0",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.18.10",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.5.0-rc.1",
    "@storybook/manager-webpack5": "^6.5.0-rc.1",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/testing-react": "^0.0.22",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv-webpack": "^8.0.0",
    "download-cli": "^1.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.1",
    "mkcert": "^1.5.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react-gsap": "^3.2.1",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.3.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --fix"
  }
}
